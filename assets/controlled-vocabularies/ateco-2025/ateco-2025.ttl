  skos:inScheme <https://w3id.org/italia/stat/controlled-vocabulary/economy/ateco-2025>;
  skos:broader :383;
  skos:notation "38.33";
  clvapit:hasRankOrder "4"^^xsd:int;
  skos:note """Sono escluse le seguenti attività:
 - gestione di impianti in cui vengono selezionati i materiali recuperabili, cfr. 38.21
 - recupero di energia mediante trattamento termico dei rifiuti, cfr. 38.22
 - decontaminazione (bonifica), disinquinamento di terreni e acque ed eliminazione di materiali tossici, cfr. 39.00""";
  skos:definition ""@en, ""@it;
  skos:narrower :38330 .

:3900 a skos:Concept, covapit:PrivateOrgActivityType;
