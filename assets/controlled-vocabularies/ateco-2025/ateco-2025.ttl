  skos:narrower :38310 .

:3832 a skos:Concept, covapit:PrivateOrgActivityType;
  skos:prefLabel "Landfilling or permanent storage"@en, "Deponierung oder Dauerlagerung von Abfällen"@de, "Conferimento in discarica o stoccaggio permanente"@it;
