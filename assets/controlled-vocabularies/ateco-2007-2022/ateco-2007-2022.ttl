  skos:broader :841;
  skos:notation "84.12";
  clvapit:hasRankOrder "4"^^xsd:int;
  skos:note """Sono escluse le seguenti attività:
- reti fognarie, raccolta, trattamento e smaltimento dei rifiuti, attività di risanamento, cfr. divisioni 37, 38, 39
 - assicurazione sociale obbligatoria, cfr. 84.30
 - istruzione, cfr. sezione P
 - assistenza sanitaria, cfr. divisione 86
