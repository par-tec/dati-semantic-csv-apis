:011 a skos:Concept, covapit:PrivateOrgActivityType;
  skos:prefLabel "COLTIVAZIONE DI COLTURE AGRICOLE NON PERMANENTI";
  skos:inScheme <https://w3id.org/italia/stat/controlled-vocabulary/economy/ateco-2007-2022>;
  skos:broader :01;
  skos:notation "01.1";
  clvapit:hasRankOrder "3"^^xsd:int;
  skos:definition "Questo gruppo include la coltivazione di colture non permanenti, ossia piante che non durano più di due stagioni agricole. È inclusa la coltivazione di queste piante per la produzione di sementi.";
  skos:narrower :0111, :0112, :0113, :0114, :0115, :0116, :0119 .

:012 a skos:Concept, covapit:PrivateOrgActivityType;
  skos:prefLabel "COLTIVAZIONE DI COLTURE PERMANENTI";
  skos:inScheme <https://w3id.org/italia/stat/controlled-vocabulary/economy/ateco-2007-2022>;
  skos:broader :01;
  skos:notation "01.2";
  clvapit:hasRankOrder "3"^^xsd:int;
